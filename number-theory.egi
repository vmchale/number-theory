legendre a p :=
  let res := modulo (power a ((p - 1) / 2)) p
   in if res = p - 1 then -1 else 1

radical n := product (unique (pF n))

jacobi n k := product (map (\p -> legendre n p) (pF k))

sumDivisors n := sum (unique (pF n))

legendre 15 17

legendre 15 13

jacobi 12 13

sumDivisors 81

radical 8
